  },
  "tokenColors": [
    {
      "scope": [
        "source",
        "variable.other",
        "support.type.property-name",
        "entity.name",
        "entity.name.type.module",
        "keyword.other.unit",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#DDDDDD"
      }
    },
    {
      "name": "function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7fc5ff"
      }
    },
    {
      "name": "class",
      "scope": [
        "entity.name.type.class",
        "keyword.operator.new",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#90eaea"
      }
    },
    {
      "name": "number",
      "scope": [
        "constant.numeric",
        "keyword.operator.arithmetic",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#b7d8f5"
      }
    },
    {
      "name": "import, if, for, return, typeof",
      "scope": ["keyword.control", "keyword.operator.expression"],
      "settings": {
        "foreground": "#BCFF8A"
      }
    },

    {
      "name": "+, -, ??, =, ===",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#fa5c5c"
      }
    },
    {
      "name": "this",
      "scope": "variable.language",
      "settings": {
        "foreground": "#eac682",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "keyword"],
      "settings": {
        "foreground": "#B9A9FF"
      }
    },
    {
      "name": ":, ,, type generics<>",
      "scope": [
        "punctuation.separator",
        "keyword.operator.assignment",
        "punctuation.definition",
        "punctuation.accessor",
        "punctuation.support",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#b7c0d1"
      }
    },
    {
      "name": "comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8A917F"
      }
    },
    {
      "name": "const, let, assignment=",
      "scope": ["storage", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#F3FD6A"
      }
    },
    {
      "name": "string",
      "scope": ["string", "string.regexp"],
      "settings": {
        "foreground": "#FD97B7"
      }
    },
    {
      "name": "args",
      "scope": ["variable.parameter", "entity.name.type.parameter"],
      "settings": { "foreground": "#f9cbff" }
    },
    {
      "name": "constant",
      "scope": [
        "variable.other.constant",
        "support.constant.property-value",
        "constant",
        "support.class.component"
      ],
      "settings": { "foreground": "#eac682" }
    },
    {
      "name": "type primitive",
      "scope": ["support.type.primitive", "support.type.builtin"],
      "settings": { "foreground": "#dd8af7" }
    }
  ],
  "semanticHighlighting": true
}
